import java.util.*;
class Solution {
    public static int climbStairs(int n) {
      int res[]=new int[n+1];
     
     res[1]=1;
     if(n==1){
        return res[n];
     }
     res[2]=2;
     for(int i=3;i<=n;i++){
        res[i]=res[i-1]+res[i-2];
     }
     return res[n];
    }
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        System.out.print(climbStairs(n));
    }
}
