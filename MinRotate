import java.util.*;
public class MinRotated{
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        int l=0,h=n-1;
        for(int i=0;i<n-1;i++){
            if(a[i]>a[i+1]){
                System.out.print(a[i+1]);
                break;
            }
        }
    }
}
