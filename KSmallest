import java.util.*;
public class KSmallest{
    public static void main(String args[]){
        Scanner s =new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        int k=s.nextInt();
        Arrays.sort(a);
        
        
        System.out.println(a[k-1]);
    }
}
