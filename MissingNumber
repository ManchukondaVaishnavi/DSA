import java.util.*;
public class MissingNumber{
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
      int n=s.nextInt();
      int a[]=new int[n-1];
      for(int i=0;i<n-1;i++){
          a[i]=s.nextInt();
      }
      int sum=0;
      for(int i=0;i<n-1;i++){
          sum+=a[i];
      } 
      int sum1=n*(n+1)/2;
      System.out.print(sum1-sum);
    }
}
