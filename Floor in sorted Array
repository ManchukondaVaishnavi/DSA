import java.util.*;
public class FloorArray{
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        int x=s.nextInt();
        int max=a[0];
        for(int i=0;i<n;i++){
            if(a[i]<=x&&max<a[i]){
                max=a[i];
            }
        }
        System.out.println(max);
    }
}
